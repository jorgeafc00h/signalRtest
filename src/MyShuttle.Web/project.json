{
  "authors": [
    "Microsoft"
  ],
  "description": "MyShuttle",
  "version": "1.0.0-beta1",
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "webroot": "wwwroot",
  "exclude": [
    "wwwroot/**/*",
    "node_modules/**/*",
    "bower_components/**/*"
  ],
  "dependencies": {
    "Kestrel": "1.0.0-beta1-*",
    "Microsoft.AspNet.Mvc": "6.0.0-beta1-*",
    "EntityFramework.InMemory": "7.0.0-beta1-*",
    "EntityFramework.SqlServer": "7.0.0-beta1-*",
    "Microsoft.AspNet.Identity.EntityFramework": "3.0.0-beta1-*",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-*",
    "Microsoft.AspNet.Security.Cookies": "1.0.0-beta1-*",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta1-*",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta1-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta1-*",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta1-*",
    "Microsoft.Framework.OptionsModel": "1.0.0-beta1-*",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta1-*",
    "Microsoft.AspNet.SignalR.Server": "3.0.0-beta1-*",
    "MyShuttle.API": "",
    "MyShuttle.Data": "",
    "MyShuttle.Model": ""
  },
  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
    "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5004"
  },
  "frameworks": {
    "aspnet50": {},
    "aspnetcore50": {}
  },
  "packExclude": [
    "node_modules/**/*",
    "bower_components/**/*"
  ],
  "scripts": {
    "postrestore": [
      "npm install"
    ]
  }
}